<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login_1</servlet-name>
        <servlet-class>login_1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login_failed</servlet-name>
        <servlet-class>login_failed</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>stock_insert</servlet-name>
        <servlet-class>stock_insert</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>stock_output</servlet-name>
        <servlet-class>stock_output</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login_1</servlet-name>
        <url-pattern>/login_1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login_failed</servlet-name>
        <url-pattern>/login_failed</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>stock_insert</servlet-name>
        <url-pattern>/stock_insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>stock_output</servlet-name>
        <url-pattern>/stock_output</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
